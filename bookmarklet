javascript:(function f(){for(var%20prices=document.getElementsByClassName("g-item-sortable"),totalPrice=0,notFound=0,i=0;i<prices.length;i++){var%20currentPrice=prices[i].getAttribute("data-price");console.log(currentPrice),currentPrice=parseFloat(currentPrice),console.log(currentPrice+"\n"),isNaN(currentPrice)||currentPrice==Number.NEGATIVE_INFINITY?notFound++:(console.log("$"+totalPrice+"%20+%20$"+currentPrice+"="),totalPrice+=currentPrice,console.log("$"+totalPrice+"\n"))}var%20prettyPrice=totalPrice.toString();console.log(prettyPrice);var%20decimal=prettyPrice.indexOf(".");prettyPrice.length>decimal+3&&-1!=decimal?prettyPrice=prettyPrice.substr(0,decimal+3):prettyPrice.length==decimal+2&&(prettyPrice+="0");var%20alertMsg="Your%20Total%20is:\n$"+prettyPrice;notFound>0&&(alertMsg+="\n\nPrices%20for%20"+notFound+"%20item(s)%20could%20not%20be%20found"),alert(alertMsg),console.log(prettyPrice);})();
